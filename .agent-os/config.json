{
  "project": {
    "name": "OOXML PPTX Processor",
    "version": "2.0.0",
    "type": "google-apps-script-cloud-platform",
    "primary_language": "javascript",
    "deployment_target": "google-cloud-run-free-tier"
  },
  "development": {
    "primary_workflow": "gas-first-development",
    "testing_framework": "playwright",
    "browser_automation": "brave",
    "deployment_method": "from-gas-ui"
  },
  "architecture": {
    "pattern": "unzip-modify-zip-pipeline",
    "components": [
      "google-apps-script",
      "google-cloud-run", 
      "fflate-library",
      "express-server"
    ],
    "data_flow": "gas-base64-cloud-processing"
  },
  "standards": {
    "code_style": "gas-compatible-es6",
    "deployment": "one-click-from-gas",
    "testing": "playwright-brave-integration",
    "documentation": "quickstart-focused"
  },
  "key_files": {
    "deployment": "src/OOXMLDeployment.js",
    "api": "lib/OOXMLJsonService.js", 
    "user_script": "DeployFromGAS.js",
    "quickstart": "QUICKSTART.md"
  },
  "testing_commands": {
    "mvp": "npm test:mvp",
    "cloud": "npm test:cloud", 
    "deployment": "npm test:deployment",
    "integration": "npm test:integration",
    "brave": "npm test:brave"
  },
  "deployment_commands": {
    "gas_setup": "setupProjectId()",
    "preflight": "showPreflightChecks()",
    "deploy": "deployToUSFreeTier()",
    "validate": "testDeployedService()"
  }
}