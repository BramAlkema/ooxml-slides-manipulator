# Environment variables for OOXML Slides testing
# Copy this file to .env and fill in your values

# Google Apps Script Project URL
GAS_PROJECT_URL=https://script.google.com/d/YOUR_PROJECT_ID/edit

# Test PowerPoint file ID from Google Drive
# Upload a .pptx file to Drive and get its ID from the URL
TEST_PPTX_FILE_ID=your_google_drive_pptx_file_id_here

# Google Service Account credentials (for CI/CD)
# Only needed for automated testing
GOOGLE_CLIENT_EMAIL=your_service_account@your_project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----\n"

# Optional: Specific test folder ID
TEST_FOLDER_ID=your_test_folder_id_here

# Playwright settings
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=60000