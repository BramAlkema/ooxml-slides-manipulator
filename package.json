{
  "name": "ooxml-pptx-processor",
  "version": "2.0.0",
  "description": "Cloud-based PPTX processing platform - unzip, modify, and zip PowerPoint files from Google Apps Script",
  "main": "src/Main.js",
  "keywords": [
    "powerpoint",
    "google-slides",
    "ooxml",
    "presentation",
    "theme",
    "colors",
    "fonts",
    "google-apps-script",
    "automation"
  ],
  "author": "OOXML Slides Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BramAlkema/ooxml-slides-manipulator.git"
  },
  "bugs": {
    "url": "https://github.com/BramAlkema/ooxml-slides-manipulator/issues"
  },
  "homepage": "https://github.com/BramAlkema/ooxml-slides-manipulator#readme",
  "scripts": {
    "test": "playwright test --project=Brave",
    "test:mvp": "node test-pptx-mvp.js",
    "test:cloud": "node test-cloud-pptx-mvp.js",
    "test:deployment": "node test-free-tier-deployment.js",
    "test:integration": "playwright test test/gas-cloud-integration.spec.js --project=Brave",
    "test:brave": "playwright test test/brave-pptx-integration.spec.js --project=Brave",
    "deploy:check": "node test-free-tier-deployment.js",
    "deploy:cli": "./deploy-simple-us.sh",
    "clasp:login": "clasp login",
    "clasp:push": "clasp push",
    "clasp:pull": "clasp pull",
    "clasp:open": "clasp open",
    "clasp:deploy": "clasp deploy",
    "install": "npm install && npx playwright install"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@playwright/test": "^1.54.2",
    "dotenv": "^16.6.1"
  },
  "type": "commonjs",
  "dependencies": {
    "fflate": "^0.8.2",
    "jszip": "^3.10.1"
  }
}
