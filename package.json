  "keywords": [
    "powerpoint",
    "google-slides",
    "ooxml",
    "presentation",
    "theme",
    "colors",
    "fonts",
    "google-apps-script",
    "automation"
  ],
  "author": "OOXML Slides Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BramAlkema/ooxml-slides-manipulator.git"
  },
  "bugs": {
    "url": "https://github.com/BramAlkema/ooxml-slides-manipulator/issues"
  },
  "homepage": "https://github.com/BramAlkema/ooxml-slides-manipulator#readme",
  "scripts": {
